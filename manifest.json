{
  "name": "Spell it out",
  "version": "1.0",
  "description": "Creates a list of expanded acronyms",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "http://www.spellitout.tech/"
      ],
      "run_at": "document_idle",
      "js": ["userToArray.js"]
    }
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_popup" : "details.html"
  },
  "manifest_version": 2
}
